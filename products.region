<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
  </head>

  <main class="content-container" role="main" data-content-field="main-content" style="width:100%">
       {squarespace.main-content}
  </main>

    <!-- combo and minify scripts when not logged in -->
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <squarespace:script src="products.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <!-- other scripts, and footer code injection -->
    {squarespace-footers}
    <script>console.log({@|json-pretty})</script>
  </body>
</html>
